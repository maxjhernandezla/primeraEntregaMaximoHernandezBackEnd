<a href="/">Index</a>
<a href="/carts/6468f3396c3a9260d3ef7a49">Cart</a>

<div>
  <div class="userData">
    <h1>Bienvenido {{user.username}}</h1>
    <p>Email: {{user.email}}</p>
    <p>Age: {{user.age}}</p>
    <p>Role: {{user.role}}</p>
    <a href="/api/sessions/logout"><button>Cerrar sesi√≥n</button></a>
  </div>
  <div class="generalContainer">
    {{#each products}}
      <div class="productContainer">
        <div class="pFront">
          <h3>{{this.title}}</h3>
          <img src={{this.image}} alt={{this.title}} />
        </div>
        <div class="pData">
          <p>{{this.category}}</p>
          <p>{{this.description}}</p>
          <p>Price: {{this.price}}</p>
          <p>Stock: {{this.stock}}</p>
        </div>
        <div class="bnContainer">
          <a href="/products/{{this._id}}">Product detail</a>
          <button class="addBtn" data-id={{this._id}}>Add to cart</button>
        </div>
      </div>
    {{/each}}
  </div>
</div>
<script
  src="
https://cdn.jsdelivr.net/npm/sweetalert2@11.7.5/dist/sweetalert2.all.min.js
"
></script>
<script src="/js/addToCart.js"></script>